{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "github": {
    "enabled": true
  },
  "functions": {
    "api": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "static": {
    "src": "/public",
    "dest": "/public"
  }
}
